{
  "$schema": "https://opencode.ai/config.json",
  "theme": "tokyonight",
  "autoshare": false,
  "autoupdate": true,
  "keybinds": {
    "input_newline": "ctrl+o",
    "messages_half_page_up": "ctrl+k",
    "messages_half_page_down": "ctrl+j",
    "messages_previous": "ctrl+h",
    "messages_next": "ctrl+l"
  }
}
